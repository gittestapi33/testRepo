        "Responsibilities.getObjectSQL": "select d.Responsibility_ID,\n    ftl.application_name || '::' || d.responsibility_name  as RESPONSIBILITY_NAME,\n    d.Description as Description,\n    ftl.application_name,\n    d.responsibility_name as name,\n    d.SECURITY_GROUP_ID,\n    ftl.APPLICATION_ID,\n    r.start_date,r.end_date\nfrom fnd_responsibility_tl d,\n    fnd_application_tl ftl,\n    fnd_responsibility r\nWHERE d.LANGUAGE='US' \n    AND r.responsibility_id = d.responsibility_id\n    AND d.application_id = ftl.application_id\n    AND d.application_id is not null\n    AND (r.start_date <= SYSDATE OR r.start_date IS null)\n    AND (r.end_date >= SYSDATE OR r.end_date IS null)\n   AND d.responsibility_id = '$(identity)'\nORDER BY Responsibility_ID",
        "Responsibilities.driverClass": "oracle.jdbc.driver.OracleDriver",
        "sourceConnected": true,
        "deltaTable": "USER_DELTA",
        "Responsibilities.useExecuteQuery": false,
        "isPermissionEnabled": false,
